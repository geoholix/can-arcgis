{{#if configured}}    
    <esri-map vm:mapOptions:from="config.mapOptions"  vm:viewOptions:from="config.viewOptions" vm:view:to="view" />
{{/if}}

{{#unless view}}
    <p style="text-align:center">
        <i class="fa fa-spinner fa-spin fa-3x fa-fw"></i>
        <span class="sr-only">Loading...</span>
    </p>
{{/unless}}