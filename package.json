{
  "name": "can-arcgis",
  "version": "1.1.0",
  "description": "A configureable mapping app built with canjs",
  "main": "index.js",
  "scripts": {
    "start": "live-server --no-browser",
    "build": "node build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "arcgis-js-api": "^4.6.1",
    "can-component": "^3.3.8",
    "can-define": "1.5.4",
    "can-observation": "3.3.6",
    "can-reflect": "1.7.3",
    "can-route": "3.2.4",
    "can-stache": "^3.14.8",
    "esri-loader": "^2.0.0",
    "flatpickr": "^4.2.4",
    "pubsub-js": "^1.5.7",
    "spectre-canjs": "^1.0.1",
    "spectre.css": "^0.5.0",
    "sweetalert2": "^6.6.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.6.1",
    "nodemon": "^1.14.3",
    "steal-css": "^1.3.1",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.1.1",
    "steal-tools": "^1.8.4"
  },
  "steal": {
    "plugins": [
