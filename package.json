    "can-component": "3.3.6",
    "can-define": "1.5.4",
    "can-observation": "3.3.6",
    "can-reflect": "1.7.3",
    "can-route": "3.2.4",
    "can-stache": "^3.14.2",
    "dgrid": "^1.2.1",
    "esri-promise": "github:roemhildtg/esri-promise",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "object-assign": "^4.1.1",
    "pubsub-js": "^1.5.7",
    "spectre.css": "^0.4.0",
    "sweetalert2": "^6.6.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.6.1",
    "steal-css": "^1.3.1",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.1.1",
    "steal-tools": "^1.8.4"
